{
    "hanzo_mcp": {
        "command": "uvx",
        "args": [
            "hanzo-mcp"
        ],
        "env": {},
        "enabled": true
    },
    "brave_search": {
        "command": "npx",
        "args": [
            "@modelcontextprotocol/server-brave-search"
        ],
        "env": {
            "BRAVE_API_KEY": "${BRAVE_API_KEY}"
        },
        "enabled": true
    },
    "github": {
        "command": "npx",
        "args": [
            "@modelcontextprotocol/server-github"
        ],
        "env": {
            "GITHUB_TOKEN": "${GITHUB_TOKEN}"
        },
        "enabled": true
    },
    "filesystem": {
        "command": "npx",
        "args": [
            "@modelcontextprotocol/server-filesystem"
        ],
        "env": {},
        "enabled": true
    },
    "youtube": {
        "command": "npx",
        "args": [
            "@anaisbetts/mcp-youtube"
        ],
        "env": {},
        "enabled": true
    },
    "tavily": {
        "command": "npx",
        "args": [
            "tavily-mcp"
        ],
        "env": {
            "TAVILY_API_KEY": "${TAVILY_API_KEY}"
        },
        "enabled": true
    },
    "web_scraper": {
        "command": "npx",
        "args": [
            "@mcp/web-scraper"
        ],
        "env": {
            "SCRAPER_TIMEOUT": "30000",
            "MAX_DEPTH": "2"
        },
        "enabled": true
    },
    "wolfram_alpha": {
        "command": "npx",
        "args": [
            "@mcp/wolfram-alpha"
        ],
        "env": {
            "WOLFRAM_ALPHA_APPID": "${WOLFRAM_ALPHA_APPID}"
        },
        "enabled": true
    },
    "calculator": {
        "command": "npx",
        "args": [
            "@mcp/calculator"
        ],
        "env": {},
        "enabled": true
    },
    "google_search": {
        "command": "npx",
        "args": [
            "@mcp/google-search"
        ],
        "env": {
            "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
            "GOOGLE_CSE_ID": "${GOOGLE_CSE_ID}"
        },
        "enabled": true
    },
    "google_drive": {
        "command": "npx",
        "args": [
            "@mcp/google-drive"
        ],
        "env": {
            "GOOGLE_API_KEY": "${GOOGLE_API_KEY}"
        },
        "enabled": true
    },
    "google_maps": {
        "command": "npx",
        "args": [
            "@mcp/google-maps"
        ],
        "env": {
            "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}"
        },
        "enabled": true
    },
    "time": {
        "command": "npx",
        "args": [
            "@mcp/time"
        ],
        "env": {},
        "enabled": true
    },
    "memory": {
        "command": "npx",
        "args": [
            "@mcp/memory"
        ],
        "env": {},
        "enabled": true
    },
    "fetch": {
        "command": "npx",
        "args": [
            "@mcp/fetch"
        ],
        "env": {},
        "enabled": true
    },
    "code_executor": {
        "command": "npx",
        "args": [
            "@mcp/code-executor"
        ],
        "env": {
            "TIMEOUT": "10000",
            "MAX_OUTPUT_SIZE": "10000"
        },
        "enabled": true
    },
    "postgres": {
        "command": "npx",
        "args": [
            "@mcp/postgres"
        ],
        "env": {
            "PG_CONNECTION_STRING": "${PG_CONNECTION_STRING}"
        },
        "enabled": false
    },
    "langchain_mcp": {
        "command": "uvx",
        "args": [
            "langchain-mcp"
        ],
        "env": {
            "OPENAI_API_KEY": "${OPENAI_API_KEY}"
        },
        "enabled": false
    },
    "custom_python_mcp": {
        "command": "python",
        "args": [
            "-m",
            "custom_mcp_module"
        ],
        "env": {},
        "enabled": false
    }
}